{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
	  "baseUrl": "src",
	  "module": "es2020",
	  "moduleResolution": "bundler",
	  "allowJs": true,
	  "checkJs": false,
	  "esModuleInterop": true,
	  "forceConsistentCasingInFileNames": true,
	  "resolveJsonModule": true,
	  "skipLibCheck": true,
	  "sourceMap": true,
	  "strict": true,
	  "paths": {
		"$server/*": ["server/*"]
	  }
	}
  }